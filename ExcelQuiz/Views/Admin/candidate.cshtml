@{
    Layout = "../Shared/_AdminLayout.cshtml";
}
<div class="row">
    <div class="col-12 col-s-12"></div>
</div>
<div class="row">
    <div class="col-1 col-s-1"></div>
    <div class="col-10 col-s-10">
        <div class="row">
            <div class="col-2 col-s-2 right"><strong>Requester Email:</strong></div>
            <div class="col-10 col-s-10 left"><input type="text" value="" /></div>
        </div>
        <div class="row">
            <div class="col-4 col-s-4"><strong>How many user you want to create</strong></div>
            <div class="col-8 col-s-8 left"><input type="text" value="" /></div>
        </div>
        <div class="row">
            <div class="col-2 col-s-2"><strong>For Quiz</strong></div>
            <div class="col-8 col-s-8">
                <select name="optQuiz" id="optQuiz" class="multi" multiple>
                    <option value="quiz1">Quiz 1</option>
                    <option value="quiz2">Quiz 2</option>
                    <option value="quiz3">Quiz 3</option>
                    <option value="quiz4">Quiz 4</option>
                    <option value="quiz5">Quiz 5</option>
                </select>
            </div>
            <div class="col-2 col-s-2"></div>
        </div>
        <div class="row">
            <div class="col-2 col-s-2"><strong>Valid From</strong></div>
            <div class="col-3 col-s-3">
                <div class="input-group date" data-provide="datepicker">
                    <input type="text" class="form-control">
                    <div class="input-group-addon">
                        <span class="glyphicon glyphicon-th"></span>
                    </div>
                </div>
            </div>
            <div class="col-7 col-s-7"></div>
        </div>
        <div class="row">
            <div class="col-2 col-s-2"><strong>Valid To</strong></div>
            <div class="col-3 col-s-3">
                <div class="input-group date" data-provide="datepicker">
                    <input type="text" class="form-control">
                    <div class="input-group-addon">
                        <span class="glyphicon glyphicon-th"></span>
                    </div>
                </div>
            </div>
            <div class="col-7 col-s-7"></div>
        </div>
        <div class="row">
                <div class="col-4 col-s-4"></div>
                <div class="col-2 col-s-2 center-block">
                    <input type="button" class="button" value="Save" />
                </div>
                <div class="col-2 col-s-2 center-block">
                    <input type="button" class="button" value="Reset" />
                </div>
                <div class="col-4 col-s-4"></div>
            </div>
        </div>
    <div class="col-1 col-s-1"></div>
</div>
